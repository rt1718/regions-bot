{
  "name": "rt1718/regions_bot",
  "description": "Телеграм бот для получения списка регионов на автомобильном номере.",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "symfony/dotenv": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Config\\": "src/Config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php public/index.php"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
